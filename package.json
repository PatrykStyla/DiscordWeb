{
	"name": "react-typescript-boilerplate",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "NODE_ENV=development cross-env INLINE_SOURCE_MAP=true webpack-dev-server --hide-modules --mode development --hot --config webpack.config.js",
		"build": "cross-env INLINE_SOURCE_MAP=false webpack --mode production --hide-modules --config webpack.prod.js",
		"tailwind": "npx tailwindcss-cli@latest build ./public/css/main.css -o /var/www/DiscordWeb/main.css -c ./tailwind.config.js",
		"tailwind-build": "NODE_ENV=production npx tailwindcss-cli@latest build ./public/css/main.css -o ./dist/main.css",
		"css": "cross-env NODE_ENV=production TAILWIND_MODE=build postcss ./public/css/main.css -o /var/www/DiscordWeb/main.css",
		"css-w": "NODE_ENV=development postcss ./public/css/main.css -o /var/www/DiscordWeb/main.css -w"
	},
